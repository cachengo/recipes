[Unit]
Description=Minio
[Service]
LimitNOFILE=500000
Environment=CI=on
Environment=MINIO_ROOT_USER=#access_key#
Environment=MINIO_ROOT_PASSWORD=#secret_key#
Environment=GROUP_ID=#group_id#
ExecStart=/usr/bin/minio server  #clusters#
Restart=always
StartLimitBurst=0
[Install]
WantedBy=multi-user.target
